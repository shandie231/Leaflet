{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es6",
    "module": "commonJS",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "jsx": "react",
  },
  "include": ["src/**/*", "server/**/*"]
}
